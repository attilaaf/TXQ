{
  "name": "txq",
  "version": "1.6.8",
  "description": "TXQ: Bitcoin Transaction Storage Queue",
  "main": "src/bootstrap/index",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/bootstrap/index.js",
    "build": "tsc -p tsconfig.json",
    "start-dev": "nodemon",
    "inspect": "nodemon --inspect src/bootstrap/index",
    "test": "mocha --require ts-node/register --require ts-jest --require source-map-support/register --watch-extensions ts --forbid-only --full-trace --bail --check-leaks --reporter list --timeout 10000 --recursive  tests",
    "jest": "jest",
    "format": "prettier --write './src/**/**/*.ts'",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "precise-commits": "precise-commits --check-only"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.16.1",
    "@types/http-server": "^0.10.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.133",
    "@types/node": "^12.0.4",
    "@types/pg": "^7.4.14",
    "@types/sinon-as-promised": "^4.0.12",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "bsv": "^1.5.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1",
    "npx": "^10.2.0",
    "prettier": "^1.17.1",
    "qs": "^6.9.4",
    "sinon": "^7.3.2",
    "sinon-as-promised": "^4.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrent-queue": "^7.0.2",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "exponential-backoff": "^3.0.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "express-prettify": "^0.1.1",
    "express-sse": "^0.5.3",
    "helmet": "^3.23.1",
    "http": "^0.0.0",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "set-tz": "^0.2.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6",
    "typedi": "^0.8.0",
    "url-join": "^4.0.1",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tslint && yarn precise-commits"
    }
  },
  "_moduleAliases": {
    "@interfaces/*": "./src/interfaces/*",
    "cfg/*": "./src/cfg/*"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatterPool/TXQ.git"
  },
  "keywords": [
    "bitcoin",
    "bsv",
    "satoshi-vision",
    "queue",
    "transaction"
  ],
  "author": "MatterPool Inc.",
  "bugs": {
    "url": "https://github.com/MatterPool/TXQ/issues"
  },
  "homepage": "https://github.com/MatterPool/TXQ#readme"
}
